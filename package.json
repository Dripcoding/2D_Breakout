	},
	"homepage": "https://github.com/JustinSHong/2D_Pong#readme",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "lint-staged"
		}
	},
	"lint-staged": {
		"./public/js/*.js": [
			"prettier --write",
			"git add"
		]
	},
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": true,
		"trailingComma": "es5",
		"useTabs": true,
		"tabWidth": 4,
		"semi": false,
		"singleQuote": true
