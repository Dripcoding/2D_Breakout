		"debug": "^3.1.0",
		"express": "^4.16.3",
		"helmet": "^3.12.1",
		"husky": "^3.0.9",
		"lint-staged": "^10.0.0-1",
		"livereload": "^0.7.0",
		"nodemon": "^1.18.2",
		"npm-run-all": "^4.1.3",
		"prettier": "^1.19.1",
		"rollup": "^0.62.0",
		"rollup-plugin-babel": "^3.0.7",
		"rollup-plugin-eslint": "^4.0.0",
		"rollup-plugin-node-resolve": "^3.3.0",
		"rollup-plugin-uglify": "^4.0.0",
		"rollup-watch": "^4.3.1",
		"socket.io": "^2.1.1"
	}
